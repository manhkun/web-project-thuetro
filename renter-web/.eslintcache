tion=\"\" onSubmit={handleSubmit}>\r\n          <FormInput\r\n            placeholder=\"Nhập tên tài khoản\"\r\n            name=\"uname\"\r\n            type=\"text\"\r\n            onChange={handleChange}\r\n            error={errors.uname}\r\n            touched={touched.uname}\r\n          ></FormInput>\r\n          <FormInput\r\n            placeholder=\"Nhập mật khẩu\"\r\n            name=\"password\"\r\n            type=\"password\"\r\n            onChange={handleChange}\r\n            error={errors.password}\r\n            touched={touched.password}\r\n          ></FormInput>\r\n          <div className=\"center\">\r\n            <Button\r\n              children=\"ĐĂNG NHẬP\"\r\n              buttonSize=\"btn--medium\"\r\n              type=\"submit\"\r\n            ></Button>\r\n            <p style={{ color: \"red\", marginTop: \"5px\" }}>{statusLogin}</p>\r\n          </div>\r\n        </form>\r\n      </Section>\r\n      <p className=\"or-register\">Hoặc</p>\r\n      <p>\r\n        Bạn chưa có tài khoản ? <Link to=\"/register\">Đăng ký</Link>\r\n      </p>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Login;\r\n","D:\\Web-Project-React\\renter-web\\src\\pages\\Profile.js",["135","136","137"],"import React, { useEffect, useState } from \"react\";\r\nimport \"./Profile.css\";\r\nimport { Link, useParams } from \"react-router-dom\";\r\n\r\nimport Section from \"../components/Section\";\r\nimport { Button } from \"../components/Button\";\r\nimport { PostedItem } from \"../components/PostItem\";\r\nimport userApi from \"../api/userApi\";\r\nimport houseApi from \"../api/houseApi\";\r\nimport { price } from \"../helper/convertPrice\";\r\nimport convertTime from \"../helper/convertTime\";\r\n\r\nfunction Profile() {\r\n  const { id } = useParams();\r\n  const [user, setUser] = useState({});\r\n  const [post, setPost] = useState([]);\r\n  const [loading, setLoading] = useState(false);\r\n\r\n  useEffect(async () => {\r\n    let user = await userApi.getInfoOwner(id);\r\n    setUser(user.data);\r\n    let post = await houseApi.getHouseByOwnerID(id);\r\n    setPost(post.data);\r\n    setLoading(true);\r\n  });\r\n  if (!loading) {\r\n    return <p>Loading</p>;\r\n  } else\r\n    return (\r\n      <div>\r\n        <Section title=\"THÔNG TIN CÁ NHÂN\" sectionStyle=\"section--validate\" />\r\n        <div className=\"info-profile-container\">\r\n          <div className=\"main-profile\">\r\n            <Section style={{ height: \"170px\" }}>\r\n              <div className=\"name-avt\">\r\n                <img src=\"/icons/profile 1.png\" alt=\"\" />\r\n                <div className=\"name\">\r\n                  <h3>{user.owner_full_name}</h3>\r\n                  <Button>Chỉnh sửa</Button>\r\n                </div>\r\n              </div>\r\n            </Section>\r\n          </div>\r\n          <div className=\"info-profile\">\r\n            <Section style={{ height: \"170px\" }}>\r\n              <div className=\"info-details\">\r\n                <div className=\"evaluate-detail\">\r\n                  <img src=\"/icons/check-list 1.png\" alt=\"\" />\r\n                  <p>Đánh giá: </p>\r\n                  <div className=\"star-voted\">\r\n                    <i className=\"fa fa-star\" aria-hidden=\"true\"></i>\r\n                    <i className=\"fa fa-star\" aria-hidden=\"true\"></i>\r\n                    <i className=\"fa fa-star\" aria-hidden=\"true\"></i>\r\n                    <i className=\"fa fa-star\" aria-hidden=\"true\"></i>\r\n                    <i className=\"fa fa-star\" aria-hidden=\"true\"></i>\r\n                  </div>\r\n                </div>\r\n                <div className=\"address-detail\">\r\n                  <img src=\"/icons/placeholder 1.png\" alt=\"\" />\r\n                  <p>\r\n                    Địa chỉ: {user.address.commune} - {user.address.district} -{\" \"}\r\n                    {user.address.province}\r\n                  </p>\r\n                </div>\r\n            